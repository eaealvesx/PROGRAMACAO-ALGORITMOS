  algoritmo "exemplo02"

  var
  idade:inteiro

  inicio
  
  escreval("digite sua idade:")
  leia(idade)

  se ((idade>=18) e (idade<50)) entao
     escreval("Voce ja tem idade para dirigir!:")
  senao
     escreval("Voce está fora do intervalo permitido!")
  fimse
  
  escreval("Acabou o programa:")
  
  fimalgoritmo
